<person id="person{{ p.id }}"
  contact="{{ p.contact }}"
  email="{{ p.email }}"
  name="{{ p.as_name }}"
  firstName="{{ p.firstName }}"
  middleName="{{ p.middleName }}"
  lastName="{{ p.lastName }}"
  privNotes="{{ p.privNotes }}"
  gender="{{ p.gender }}"
  complete="{{ p.complete }}"
  distEmail="{{ p.distEmail }}"
  recordingOkay="{{ p.recordingOkay }}"
  badge="{{ p.badge }}"
  memnum="{{ p.memnum }}"
  notes="{{ p.pubNotes }}"
>
  <tag_uses>
  {% for t in p.tags.all %}
  {{ t.as_xml }}
  {% endfor %}
  </tag_uses>
  <availability>
  {% for a in p.availability.all %}
  {{ a.as_xml }}
  {% endfor %}
  </availability>
</person>

